@model CarDealershipASPNETMVC.Models.CarAccessoriesProductGroupModel
@{
}
<br />
<h2>Edit Kraftstoff</h2>

<form method="post" action="Edit" class="mt-5">
     <div class="row-mb3">
        <label class="col-sm3 col-form-label">@Html.DisplayNameFor(model => model.CAPGId)</label>
        <div class="col-sm6">
            <input type="number" class="form-control" asp-for="CAPGId" readonly>
        </div>
    </div>
    <div class="row-mb3">
        <label class="col-sm3 col-form-label">@Html.DisplayNameFor(model => model.CAPGName)</label>
        <div class="col-sm6">
            <input type="text" class="form-control" asp-for="CAPGName">
        </div>
    </div>
<br />
    <button type="submit" class="btn-primary">Submit</button>
</form>