@model CarDealershipASPNETMVC.Models.GearboxModel
@{
}
<br />
<h2>Edit Getriebe</h2>

<form method="post" action="Edit" class="mt-5">
     <div class="row-mb3">
        <label class="col-sm3 col-form-label">@Html.DisplayNameFor(model => model.GearboxId)</label>
        <div class="col-sm6">
            <input type="number" class="form-control" asp-for="GearboxId" readonly>
        </div>
    </div>
    <div class="row-mb3">
        <label class="col-sm3 col-form-label">@Html.DisplayNameFor(model => model.GearboxName)</label>
        <div class="col-sm6">
            <input type="text" class="form-control" asp-for="GearboxName">
        </div>
    </div>
<br />
    <button type="submit" class="btn-primary">Submit</button>
</form>