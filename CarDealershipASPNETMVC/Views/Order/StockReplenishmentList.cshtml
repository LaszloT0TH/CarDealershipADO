<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.SRLId)</th>
            <th>@Html.DisplayNameFor(model => model.ProductId)</th>
            <th>@Html.DisplayNameFor(model => model.ProductName)</th>
            <th>@Html.DisplayNameFor(model => model.OrderedStatus)</th>
            <th>@Html.DisplayNameFor(model => model.SRLTimeStamp)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (StockReplenishmentListModel SRL in Model)
        {
            <tr>
                <td>@SRL.SRLId</td>
                <td>@SRL.ProductId</td>
